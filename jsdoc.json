{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": "db",
        "includePattern": ".js$",
        "excludePattern": "node_mudules/|docs"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "destination": "./docs",
        "template": "nap-db-doc-template"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    }
}